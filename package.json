{
  "name": "wfto",
  "version": "1.0.0",
  "private": true,
  "description": "Russian clone for a web site wfto.com",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "ajv": "^6.5.0",
    "autoprefixer": "8.3.x",
    "browser-sync": "2.23.x",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-rigger": "^0.5.8",
    "gulp-sass": "4.0.x",
    "gulp-svgstore": "^6.1.1",
    "gulp-uglify": "^3.0.1",
    "gulp-wait": "0.0.2",
    "gulp-webp": "^3.0.0",
    "node-normalize-scss": "^8.0.0",
    "posthtml-include": "^1.1.0",
    "run-sequence": "^2.2.1",
    "stylelint": "9.2.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli -x 'app/js/picturefill.js', 'app/js/svgxuse.js'",
    "stylelint": "stylelint \"app/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "app/*.html",
    "app/js/**/*.js",
    "app/img/**/*.svg",
    "app/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.11"
  },
  "dependencies": {
    "imagemin": "^5.3.1",
    "imagemin-webp": "^4.1.0",
    "node-modules": "^1.0.1",
    "normalize.css": "^8.0.1",
    "npm": "^5.8.0"
  },
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaudVrn/orion.git"
  },
